{
  "name": "metricador",
  "description": "Core library to add performance metrics to your Node.js application.",
  "version": "0.3.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/ovidiubute/metricador.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-codecov": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec && ./node_modules/codecov/bin/codecov",
    "lint": "jshint *.js"
  },
  "keywords": [
    "metrics",
    "performance",
    "counter",
    "histogram",
    "graphite",
    "zabbix"
  ],
  "author": "Ovidiu Bute <ovidiu.bute@gmail.com>",
  "license": {
    "type": "MIT",
    "url": "https://github.com/ovidiubute/metricador/blob/master/LICENSE"
  },
  "engines": {
    "node": ">=0.12.x"
  },
  "dependencies": {
    "graphite": "0.0.6",
    "zabbix-sender": "^0.2.0"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.1",
    "mocha": "^2.3.4",
    "sinon": "^1.17.3"
  }
}
